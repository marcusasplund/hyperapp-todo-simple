    "ignore": [
      "dist/",
      "release/"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-sw-cache": "^0.3.1",
    "babel-eslint": "^10.1.0",
    "sass": "^1.26.1",
    "standard": "*"
